    -->
    <entry>
        <string>au_crawlrules</string>
        <list>
            <!--https://cdn.publisher.gn1.link/residenciapediatrica.com.br/pdf/v1n1a07.pdf-->
            <string>1,"https://cdn.publisher.gn1.link/residenciapediatrica.com.br/pdf/"</string>
            <string>1,"https://cdn.gn1.link/residenciapediatrica/.*\.(css|gif|jpe?g|js|png)"</string>
            <string>1,"https://s3-sa-east-1.amazonaws.com/.*\.pdf"</string>
            <string>1,"%s(detalhes|exportar-pdf)/", base_url</string>
            <string>1,"%sexportar-citacao/\\d+/ris", base_url</string>
        </list>
    </entry>
