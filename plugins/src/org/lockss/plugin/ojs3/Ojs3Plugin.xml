  <entry>
    <string>au_repair_from_peer_if_missing_url_pattern</string>
    <list>
      <string>/(plugins/(generic|themes)|lib/pkp|public/journals)/.+[.](css|eot|gif|png|jpe?g|js|svg|ttf|woff)([?]((itok|v)=)?[^&amp;]+)?$</string>
    </list>
  </entry>
